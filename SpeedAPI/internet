local blacklist = {"https://pornhub.com"}

_http = http

local function checkForBlacklist(url)
	for _, urlik in pairs(blacklist) do
		if url == urlik then
			return true
		end
	end

	return false
end

function get(url)
	if not checkForBlacklist(url) then
		return _http.get(url)
	end
end

function post(url)
	if not checkForBlacklist(url) then
		return _http.post(url)
	end
end