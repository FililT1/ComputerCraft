local URLblacklist = {"https://github.com/"}

_http = http

local function checkForBlacklist(url)
	for _, urlik in pairs(URLblacklist) do
		if url == urlik then
			return true
		end
	end

	return false
end

http.get = function(url)
	if not checkForBlacklist(url) then
		return _http.get(url)
	end
end

http.post = function(url)
	if not checkForBlacklist(url) then
		return _http.post(url)
	end
end